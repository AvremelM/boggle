{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./js",
    "incremental": true,
    "target": "es6",
    "tsBuildInfoFile": ".tsbuildinfo",
    "noEmitOnError": false,
    "target": "es2015",
    "lib": ["es2017", "dom"]
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "js",
    "wip"
  ]
}
